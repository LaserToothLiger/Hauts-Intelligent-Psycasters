  <PawnKindDef>
    <defName>HVPAA_CourtPsycaster</defName>
    <label>court psycaster</label>
    <race>Human</race>
    <defaultFactionType>Empire</defaultFactionType>
    <maxGenerationAge>90</maxGenerationAge>
    <backstoryCryptosleepCommonality>0.08</backstoryCryptosleepCommonality>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>ImperialRoyal</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
    <royalTitleChance>0</royalTitleChance>
    <isFighter>false</isFighter>
    <chemicalAddictionChance>0.1</chemicalAddictionChance>
    <invNutrition>2.55</invNutrition>
    <itemQuality>Good</itemQuality>
    <inventoryOptions>
     <skipChance>0.70</skipChance>
     <subOptionsTakeAll>
       <li>
         <thingDef>GoJuice</thingDef>
         <countRange>1~5</countRange>
       </li>
     </subOptionsTakeAll>
   </inventoryOptions>
    <combatPower>200</combatPower>
    <weaponMoney>99999</weaponMoney>
    <weaponTags Inherit="False">
      <li>EltexStaff</li>
    </weaponTags>
    <requiredWorkTags>
      <li>Violent</li>
    </requiredWorkTags>
    <apparelRequired>
      <li>Apparel_EltexSkullcap</li>
	  <li>Apparel_PsyfocusRobe</li>
	  <li>Apparel_PsyfocusVest</li>
      <li>Apparel_ShieldBelt</li>
    </apparelRequired>
    <apparelMoney>1000~2222</apparelMoney>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <apparelDisallowTags>
      <li>Western</li>
    </apparelDisallowTags>
    <apparelColor>(70,124,145)</apparelColor>
    <specificApparelRequirements>
      <li>
        <bodyPartGroup>Torso</bodyPartGroup>
        <apparelLayer>OnSkin</apparelLayer>
        <stuff>Hyperweave</stuff>
      </li>
      <li>
        <bodyPartGroup>Legs</bodyPartGroup>
        <stuff>Hyperweave</stuff>
      </li>
    </specificApparelRequirements>
    <techHediffsChance>0.15</techHediffsChance>
    <techHediffsMoney>1000~4000</techHediffsMoney>
    <techHediffsTags>
      <li>Advanced</li>
      <li>ImplantEmpireCommon</li>
	  <li>ImplantEmpireRoyal</li>
    </techHediffsTags>
    <skills>
      <li>
        <skill>Melee</skill>
        <range>6~20</range>
      </li>
    </skills>
    <initialWillRange>10~12</initialWillRange>
    <initialResistanceRange>90~110</initialResistanceRange>
    <xenotypeSet>
      <xenotypeChances>
        <Genie MayRequire="Ludeon.RimWorld.Biotech">0.05</Genie>
      </xenotypeChances>
    </xenotypeSet>
    <useFactionXenotypes>false</useFactionXenotypes>
    <disallowedTraits>
      <li>Nudist</li>
    </disallowedTraits>
    <humanPregnancyChance>0</humanPregnancyChance>
	<modExtensions>
	  <li Class="HVPAA.AddedSpecPsycasters">
		<domesticChance>0.25</domesticChance>
		<pointCost>20</pointCost>
		<domesticCount>2</domesticCount>
		<minPsylinkLevel>4~6</minPsylinkLevel>
		<bonusPsycasts>3</bonusPsycasts>
		<combatCastIntervalOverride>90</combatCastIntervalOverride><!--they're valuable for their psycasting, so they should psycast rapidly and frequently in combat. It also helps them survive longer since they aren't armored well for melee combat, and this usually slows down the rate at which they approach foes-->
	  </li>
	</modExtensions>
  </PawnKindDef>
  
