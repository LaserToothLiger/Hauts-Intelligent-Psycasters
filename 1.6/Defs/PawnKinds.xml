<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--Shattered Empire-->
  <PawnKindDef ParentName="ImperialFighterBase">
    <defName>HVPAA_PersonaTemplar</defName>
    <label>persona templar</label>
    <royalTitleChance>0.1</royalTitleChance>
    <maxGenerationAge>50</maxGenerationAge>
    <apparelMoney>7000~10000</apparelMoney>
	<weaponMoney>3000~6000</weaponMoney>
    <techHediffsMoney>1000~5000</techHediffsMoney>
    <techHediffsMaxAmount>6</techHediffsMaxAmount>
    <allowRoyalRoomRequirements>False</allowRoyalRoomRequirements>
    <titleSelectOne>
      <li>Freeholder</li>
      <li>Yeoman</li>
    </titleSelectOne>
    <combatPower>300</combatPower>
    <weaponTags>
      <li>Bladelink</li>
    </weaponTags>
    <apparelColor>(250,250,250)</apparelColor>
    <apparelRequired>
      <li>Apparel_ArmorHelmetCataphract</li>
      <li>Apparel_ArmorCataphract</li>
      <li>Apparel_ShieldBelt</li>
    </apparelRequired>
    <specificApparelRequirements Inherit="False">
      <li>
        <bodyPartGroup>Torso</bodyPartGroup>
        <apparelLayer>OnSkin</apparelLayer>
        <stuff>Synthread</stuff>
      </li>
      <li>
        <bodyPartGroup>Legs</bodyPartGroup>
        <apparelLayer>OnSkin</apparelLayer>
        <stuff>Synthread</stuff>
      </li>
    </specificApparelRequirements>
    <techHediffsTags>
      <li>Advanced</li>
      <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <techHediffsDisallowTags>
      <li>PainCauser</li>
    </techHediffsDisallowTags>
    <techHediffsRequired>
      <li>ArmorskinGland</li>
    </techHediffsRequired>
    <skills>
      <li>
        <skill>Melee</skill>
        <range>8~14</range>
      </li>
    </skills>
    <disallowedTraits>
      <li>Wimp</li>
      <li>Nudist</li>
      <li>BodyPurist</li>
	  <li MayRequire="Hautarche.HautsTraits">HVT_Sniper</li>
    </disallowedTraits>
    <requiredWorkTags>
      <li>Violent</li>
    </requiredWorkTags>
    <initialWillRange>8~10</initialWillRange>
    <initialResistanceRange>40~75</initialResistanceRange>
    <useFactionXenotypes>false</useFactionXenotypes>
    <xenotypeSet>
      <xenotypeChances>
        <Neanderthal MayRequire="Ludeon.RimWorld.Biotech">0.03</Neanderthal>
        <!--<Genie MayRequire="Ludeon.RimWorld.Biotech">0.03</Genie>-->
      </xenotypeChances>
    </xenotypeSet>
	<modExtensions>
	  <li Class="HVPAA.AddedSpecPsycasters">
		<domesticChance>0.35</domesticChance><!--the psycaster commonality setting can't bring this above 90%-->
		<pointCost>10</pointCost>
		<domesticCount>1~2</domesticCount>
		<minPsylinkLevel>1~2</minPsylinkLevel>
		<bonusPsycasts>0</bonusPsycasts>
		<raidChance>0.2</raidChance>
		<raidCount>1~2</raidCount>
	  </li>
	  <li Class="HVPAA.StrikeSquaddie" />
	</modExtensions>
  </PawnKindDef>
